query GetProfile($lang: Lang) {
  profile(lang: $lang) {
    id
    name
    image
    birthDate
    businessTitle
    speciality
    accounts {
      ...Account
    }
    email
    introduction
    message
    skills {
      ...Skill
    }
    educationalBackgrounds {
      ...EducationalBackground
    }
    workHistories {
      ...WorkHistory
    }
    projects {
      ...Project
    }
    message
  }
}
