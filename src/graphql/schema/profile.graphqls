type Query {
  profile(lang: Lang): Profile!
}

type Profile {
  id: ID!
  name: String!
  image: String
  birthDate: String!
  businessTitle: String!
  speciality: String!
  accounts: [Account!]!
  email: String!
  message: String
  skills: [Skill!]!
  educationalBackgrounds: [EducationalBackground!]!
  workHistories: [WorkHistory!]!
  projects: [Project!]!
}
